{
    "extends": "eslint-config-mosaic",
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "flowtype"
    ],
    "rules": {
        "no-floating-decimal": 1
    }
}
